{
  "manifest_version": 2,
  "name": "qrcode",
  "version": "1.0",
  "description": "生成页面二维码",
  "icons": {
    "48": "images/api.png"
  },
  "page_action": {
    "default_icon": "images/api.png",
    "default_title": "页面二维码",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "default_popup":"index.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ]
}